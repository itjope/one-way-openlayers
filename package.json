{
  "name": "one-way-openlayers",
  "version": "0.2.0",
  "description": "One way dataflow wrapper for OpenLayers maps",
  "main": "lib/index.js",
  "scripts": {
    "test": "./node_modules/karma/bin/karma start",
    "start": "webpack-dev-server --watch --inline",
    "flow": "./node_modules/flow-bin/cli.js",
    "deploy": "babel src --presets babel-preset-es2015 --out-dir lib",
    "prepublish": "npm run deploy"
  },
  "files": ["lib"],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/itjope/one-way-openlayers.git"
  },
  "keywords": [
    "map",
    "openlayers"
  ],
  "author": "itjope",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/itjope/one-way-openlayers/issues"
  },
  "homepage": "https://github.com/itjope/one-way-openlayers#readme",
  "dependencies": {
    "diff-json": "^0.1.11",
    "fast-json-patch": "^1.1.8",
    "lodash": "^4.17.4",
    "openlayers": "^4.0.1",
    "proj4": "^2.3.17"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "case-sensitive-paths-webpack-plugin": "^1.1.4",
    "flow-bin": "^0.38.0",
    "html-webpack-plugin": "^2.28.0",
    "jasmine-core": "^2.5.2",
    "karma": "^1.4.1",
    "karma-chrome-launcher": "^2.0.0",
    "karma-jasmine": "^1.1.0",
    "karma-webpack": "^2.0.2",
    "webpack": "^2.2.0",
    "webpack-dev-server": "^2.3.0"
  }
